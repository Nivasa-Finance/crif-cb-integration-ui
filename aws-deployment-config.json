{
  "frontend": {
    "domain": "nivasa.io",
    "subdomain": "",
    "fullUrl": "https://nivasa.io",
    "region": "ap-south-1",
    "buildSettings": {
      "buildCommand": "npm run build",
      "outputDirectory": "dist",
      "nodeVersion": "18"
    }
  },
  "backend": {
    "domain": "api.nivasa.io",
    "apiPath": "",
    "fullApiUrl": "https://api.nivasa.io",
    "region": "ap-south-1",
    "apiGateway": {
      "restApiName": "nivasa-crif-api",
      "stageName": "prod",
      "endpointType": "REGIONAL"
    }
  },
  "environmentVariables": {
    "production": {
      "VITE_API_BASE_URL": "https://api.nivasa.io",
      "VITE_APP_TITLE": "Nivasa Navigator",
      "VITE_APP_ENV": "production"
    },
    "staging": {
      "VITE_API_BASE_URL": "https://staging-api.nivasa.io",
      "VITE_APP_TITLE": "Nivasa Navigator (Staging)",
      "VITE_APP_ENV": "staging"
    }
  }
} 